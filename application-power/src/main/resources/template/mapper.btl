<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="${basePackage}.dao.${entitySimpleName}Dao">

    <resultMap type="${entitySimpleName}" id="${entitySimpleName}Result">
   ${resultMap}
    </resultMap>

    <!--保存数据selectKey若启用则添加成功后id会自动返回-->
	<insert id="save" parameterType="${entitySimpleName}">
	<!-- <selectKey resultType="int" keyProperty="id" order="AFTER">-->
		<!--	SELECT LAST_INSERT_ID()-->
	<!--	 </selectKey>-->
		${insertSql}
	</insert>
    <!--更新数据-->
    <update id="update" parameterType="${entitySimpleName}">
    	${updateSql}
    	where id = #{id}
    </update>
    <!--删除数据-->
	<delete id="delete" parameterType="int">
		delete from ${tableName} where id = #{id}
	</delete>

    <!--根据id返回一条记录-->
	<select id="queryById" parameterType="int" resultType="${entitySimpleName}">
		${selectSql}
		 where id=#{id}
	</select>

	<!--查询全部记录-->
    <select id="queryAll" resultMap="${entitySimpleName}Result">
        ${selectSql}
    </select>

</mapper>